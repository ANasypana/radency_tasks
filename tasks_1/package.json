{
  "name": "Notes",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "devserver": "live-server",
    "start": "npm-run-all build:css devserver",
    "compile:sass": "node-sass src/styles/scss/index.scss src/styles/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/styles/css/style.comp.css -o src/styles/css/style.prefix.css",
    "compress:css": "node-sass src/styles/css/style.prefix.css src/styles/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Alla Nasypana",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.9",
    "concat": "^1.0.3",
    "live-server": "^1.2.2",
    "node-sass": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  }
}
